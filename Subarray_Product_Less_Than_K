class Solution {
    public int numSubarrayProductLessThanK(int[] arr, int k) {
          int si = 0;
        int ei = 0;
        int  p = 1;
        int ans = 0;
        while(ei<arr.length){
            p = p*arr[ei];
            while(p >= k && si <= ei){
            p = p / arr[si];
            si++;
            }
            ans = ans + ei-si+1;
            ei++;
        }
        return ans;
    }
}
