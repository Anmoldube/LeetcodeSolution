class Solution {
    public int maxProfit(int[] prices) {
      if(prices.length == 1)
      return 0;
      int prof = 0;
      int min = prices[0];
      int max = 0;
        for(int stockprices:prices)
        {
            min = Math.min(min,stockprices);
            prof = stockprices-min;
            max = Math.max(prof ,max);

        }
        return max;
    }
}
