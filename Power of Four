class soluion{
public boolean isPowerOfThree(int n){
if(n%4 != 0) return false;
while(n>=4){
n = n/4;
}
return n==1
}
}

class Solution {
    public boolean isPowerOfFour(int n) {
        while(n>=4){
        if(n%4 != 0) return false;
          n = n/4;  
        }
        return n==1;
    }
}

     
